{
  "kind": "collectionType",
  "collectionName": "sell_games",
  "info": {
    "name": "SellGames",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "price": {
      "type": "float",
      "required": true
    },
    "remarks": {
      "type": "text",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "CANCELLED",
        "CONFIRMED",
        "NOT_COMPLETE",
        "DISPATCHED"
      ],
      "default": "NOT_COMPLETE"
    },
    "negotiable": {
      "type": "boolean",
      "required": true
    },
    "location": {
      "type": "component",
      "repeatable": false,
      "component": "types.location",
      "required": true
    },
    "user": {
      "private": true,
      "plugin": "users-permissions",
      "model": "user"
    },
    "games": {
      "collection": "games"
    },
    "game_conditions": {
      "type": "component",
      "repeatable": true,
      "component": "types.game-condition"
    }
  }
}
