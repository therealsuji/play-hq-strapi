{
  "kind": "collectionType",
  "collectionName": "sell_games",
  "info": {
    "name": "SellGames",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "integer"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "remarks": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "CANCELLED",
        "NOT_COMPLETE",
        "DISPATCHED"
      ],
      "default": "NOT_COMPLETE"
    },
    "negotiable": {
      "type": "boolean",
      "required": true
    },
    "location": {
      "type": "component",
      "repeatable": false,
      "component": "types.location",
      "required": true
    },
    "games": {
      "type": "component",
      "repeatable": true,
      "component": "games.game-status",
      "required": true,
      "min": 1
    }
  }
}
