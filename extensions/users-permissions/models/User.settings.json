{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "phone_number": {
      "type": "string"
    },
    "pref_genres": {
      "type": "component",
      "repeatable": true,
      "component": "types.genre"
    },
    "pref_release_dates": {
      "type": "component",
      "repeatable": true,
      "component": "types.release-date"
    },
    "pref_platforms": {
      "type": "component",
      "repeatable": true,
      "component": "types.platform"
    },
    "location": {
      "type": "component",
      "repeatable": false,
      "component": "types.location"
    },
    "notification_token": {
      "type": "text"
    },
    "name": {
      "type": "string"
    }
  }
}
